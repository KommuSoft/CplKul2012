function foo(n)
	if n>0 then
		return foo(n-1)
	else
		return 0
	end
end

x = foo(1000000000)
print(x)

public class TailCallOptimizationTest {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        long test = foo(10000);
    }
    
    public static long foo(long n){
        if(n>0){
            return foo(n-1);
        }else{
            return 0;
        }
    }
}

